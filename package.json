{
  "name": "elisabethkoch.eu",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve:ssr": "node dist/elisabethkoch.eu/server/main.js",
    "build:ssr": "ng build && ng run elisabethkoch.eu:server",
    "dev:ssr": "ng run elisabethkoch.eu:serve-ssr",
    "prerender": "ng run elisabethkoch.eu:prerender",
    "watch": "ng build --watch --configuration development",
    "build-prod": "ng build --configuration development",
    "test": "ng test",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/de.json ./src/assets/i18n/en.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,de}.json --clean --format json",
    "get-cpu-cores": "node get-cpu-cores.js",
    "serve:ssr:elisabethkoch.eu": "node dist/elisabethkoch.eu/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.7",
    "@angular/cdk": "^19.1.5",
    "@angular/common": "^19.1.7",
    "@angular/compiler": "^19.1.7",
    "@angular/core": "^19.1.7",
    "@angular/forms": "^19.1.7",
    "@angular/material": "^19.1.5",
    "@angular/platform-browser": "^19.1.7",
    "@angular/platform-browser-dynamic": "^19.1.7",
    "@angular/platform-server": "^19.1.7",
    "@angular/router": "^19.1.7",
    "@angular/ssr": "^19.1.8",
    "@iframe-resizer/child": "^5.3.2",
    "@iframe-resizer/parent": "^5.3.2",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@nguniversal/builders": "^16.2.0",
    "@nguniversal/express-engine": "^8.2.6",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@vercel/analytics": "^1.4.1",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mongodb": "^6.3.0",
    "ngx-spinner": "^17.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "~19.1.8",
    "@angular/compiler-cli": "^19.1.7",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.0.0",
    "angular-cli-ghpages": "^1.0.7",
    "browser-sync": "^3.0.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.7.3"
  }
