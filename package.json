{
  "name": "elisabethkoch.eu",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build-prod": "ng build --configuration development",
    "build:ssr": "ng build --configuration=production && ng run elisabethkoch.eu:server",
    "serve:ssr": "node dist/server/main.js",
    "test": "ng test",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/de.json ./src/assets/i18n/en.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,de}.json --clean --format json",
    "get-cpu-cores": "node get-cpu-cores.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/platform-server": "^19.1.7",
    "@angular/router": "^17.3.11",
    "@angular/ssr": "^17.3.8",
    "@iframe-resizer/child": "^5.3.2",
    "@iframe-resizer/parent": "^5.3.2",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@vercel/analytics": "^1.4.1",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mongodb": "^6.3.0",
    "ngx-spinner": "^17.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.4",
    "@angular/cli": "~17.3.8",
    "@angular/compiler-cli": "^17.3.11",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@types/jasmine": "~4.0.0",
    "angular-cli-ghpages": "^1.0.7",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.4.5"
  }
}
